{
  "_version": "1.65.0",
  "sap.app": {
    "id": "profile",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.1",
      "toolsId": "aa4dbf91-71b8-459c-b43b-c11d4376fd93"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "profile.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "profile.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": ":?query:",
          "target": [
            "TargetApp"
          ]
        },
        {
          "name": "RouteAboutMe",
          "pattern": "aboutMe/",
          "target": [
            "TargetAboutMe"
          ]
        },
        {
          "name": "RouteLaboral",
          "pattern": "laboral/{company}",
          "target": [
            "TargetLaboral"
          ]
        }
      ],
      "targets": {
        "TargetApp": {
          "id": "App",
          "name": "App"
        },
        "TargetAboutMe": {
          "id": "AboutMe",
          "name": "AboutMe"
        },
        "TargetLaboral": {
          "id": "Laboral",
          "name": "Laboral"
        }
      }
    },
    "rootView": {
      "viewName": "profile.view.App",
      "type": "XML",
      "id": "AppRoot",
      "async": true
    }
  }
}
